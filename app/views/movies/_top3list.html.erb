<% if @top_sixties.count > 0 %>
	<table>
		<tr>
			<th>Rank</th>
			<th>Title</th>
			<th>Points</th>
			<th>IMDB</th>
			<th>Tomatoes</th>
			<th>BoxOffice</th>
		</tr>

		<% @top_sixties.each_with_index do |movie, index| %>
			<tr>
				<td><%= index + 1 %></td>
				<td><%= movie.title %></td>
				<td><%= movie.flixratingspoints %></td>
				<td><%= movie.imdbranking %></td>
				<td><%= movie.tomatorating %></td>
				<td><%= movie.takings %></td>
			</tr>
		<% end %>
	</table>
<% end %>